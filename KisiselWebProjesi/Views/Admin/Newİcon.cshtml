@model KisiselWebProjesi.Models.Classes.icons
@{
    ViewBag.Title = "NewIcon";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Add New Icon</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.icon)
    @Html.TextBoxFor(x => x.icon, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.link)
    @Html.TextBoxFor(x => x.link, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Add Icon</button>
</form>
